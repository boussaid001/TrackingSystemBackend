{
  "name": "packages-microservice",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node ./src",
    "start:local": "nodemon",
    "start:dev": "cp src/environment/.dev.env .env && npm start",
    "start:prod": "",
    "format": "prettier --write \"src/**/*.ts\"",
    "setup:apm": "bash ./bin/apm.setup.sh",
    "setup:db": "bash ./bin/db.setup.sh && bash ./bin/db.migrations.sh ",
    "run:build": "node build/src",
    "test:build": "cp src/environment/.local.env build/.env && npm run run:build ",
    "build": "rm -rf build && tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\" \"npm run test:build\"",
    "test": "jest --verbose false --testPathIgnorePatterns '.history/'",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix"
  },
  "engines": {
    "node": ">= 18.12"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1584.0",
    "axios": "^1.6.7",
    "color": "^4.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "elastic-apm-node": "^4.5.0",
    "express": "^4.19.1",
    "joi": "^17.11.0",
    "moment": "^2.30.1",
    "mongodb": "^6.4.0",
    "mongoose": "^8.2.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^29.5.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-api-utils": "^1.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
